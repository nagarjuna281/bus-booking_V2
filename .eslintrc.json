{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "consistent-return": "off",
    "prefer-const": "error",
    "no-unused-vars": "error",
    "no-undef": "error"
  }
}
